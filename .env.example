# BLACKBOX AI - Configuración de Entorno
# Copia este archivo como .env y configura tus claves API

# Puerto del servidor
PORT=8000

# Configuración de IA
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Base de datos (opcional)
MONGODB_URI=mongodb://localhost:27017/blackbox-ai
DATABASE_URL=postgresql://localhost:5432/blackbox-ai

# Configuración de seguridad
JWT_SECRET=your_jwt_secret_here
BCRYPT_ROUNDS=12

# Configuración de email (opcional)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_email_password

# Configuración de almacenamiento
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=50MB

# Configuración de desarrollo
NODE_ENV=development
DEBUG=true

# APIs adicionales (opcional)
REPLICATE_API_TOKEN=your_replicate_token_here
HUGGINGFACE_API_KEY=your_huggingface_key_here
